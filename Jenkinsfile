pipeline {
    agent any

    stages {
        stage('Start') {
            steps {
                echo 'Starting process...'
            }
        }
        stage('Build') {
            steps {
                echo 'Build was successiful.'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Job is being deployed...'
            }
        }
        stage('Test') {
            steps {
                echo 'Job is under testing...'
            }
        }
        stage('Release') {
            steps {
                echo 'Job is now released.'
            }
        }
    }
}
